<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="member">
<insert id="save" parameterType="MDTO">
    insert into member1_table(memberId,memberPassword,memberName,memberEmail,memberMobile,memberProfileName)
    values (#{memberId},#{memberPassword},#{memberName},#{memberEmail},#{memberMobile},#{memberProfileName})
</insert>
    <select id="duplicateCheck" parameterType="String" resultType="MDTO">
        select * from member1_table where memberId=#{memberId}
    </select>
    <select id="loginForm" parameterType="MDTO" resultType="MDTO">
        select * from member1_table where memberId=#{memberId} and memberPassword=#{memberPassword}
    </select>
    <select id="findAll" resultType="MDTO">
        select * from member1_table;
    </select>
</mapper>














